#include <iostream>
#include <string>
#include <vector>

struct BookT {
    string title;
    vector<string> authors;
    string catalogNumber;
    vector<string> subjectHeadings;
    string publisher;
    int yearOfPublication;
    bool isCirculating;
}
void SearchBySubject(const:vector<Book>& library, const string& subject) {
    cout << "Books with the subject '" << subject << "':" << endl;
    for (const Book& book : library) {
        for (const std::string& heading : book.subjectHeadings) {
            if (heading == subject) {
                cout << "Title: " << book.title << endl;
                if (!book.authors.empty()) {
                    cout << "Author: " << book.authors[0] << endl;
                }
                cout << "Catalog Number: " << book.catalogNumber << endl;
                cout << endl;
                break;
            }
        }
    }
}